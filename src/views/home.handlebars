<h1>Productos</h1>

<div>
    <ul>
        {{#each products}}
            <li>
                <h2>{{this.title}}</h2> 
                <img src={{this.thumbnails}}>
                <p>{{this.description}} </p> 
                <p>{{this.price}}</p> 
            </li>
        {{/each}}
    </ul>
</div>